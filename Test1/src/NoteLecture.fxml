<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="800.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="application.NoteLectureController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <AnchorPane prefHeight="776.0" prefWidth="800.0">
         <children>
            <Label layoutX="14.0" layoutY="24.0" text="Week" />
            <Label layoutX="15.0" layoutY="63.0" text="Date" />
            <Label layoutX="14.0" layoutY="105.0" text="Groups" />
            <Label layoutX="400.0" layoutY="24.0" text="Staff" />
            <Label layoutX="399.0" layoutY="63.0" text="Module" />
            <Label layoutX="399.0" layoutY="105.0" text="Subject" />
            <TextField fx:id="Week" layoutX="53.0" layoutY="20.0" prefHeight="25.0" prefWidth="229.0" promptText="EG 2" />
            <TextField fx:id="Date" layoutX="53.0" layoutY="59.0" prefHeight="25.0" prefWidth="229.0" promptText="EG 12/02/2016" />
            <TextField fx:id="Groups" layoutX="53.0" layoutY="101.0" prefHeight="25.0" prefWidth="229.0" promptText="Eg A1-A3" />
            <TextField fx:id="Staff" layoutX="454.0" layoutY="20.0" prefHeight="25.0" prefWidth="229.0" promptText="EG DR Todd" />
            <TextField fx:id="Module" layoutX="454.0" layoutY="59.0" prefHeight="25.0" prefWidth="229.0" promptText="EG MED 101" />
            <TextField fx:id="Subject" layoutX="454.0" layoutY="101.0" prefHeight="25.0" prefWidth="229.0" promptText="EG Medicine" />
            <ListView fx:id="TableLectures" layoutX="14.0" layoutY="155.0" onMouseClicked="#TableClick" prefHeight="493.0" prefWidth="455.0" />
            <TextArea fx:id="Note" layoutX="483.0" layoutY="248.0" prefHeight="322.0" prefWidth="260.0" />
            <Button fx:id="Upload" layoutX="503.0" layoutY="600.0" mnemonicParsing="false" onAction="#Upload" text="Upload Note" />
            <Button fx:id="Cancel" layoutX="646.0" layoutY="600.0" mnemonicParsing="false" onAction="#Cancel" text="Cancel" />
            <Label fx:id="SelectedLecture" layoutX="483.0" layoutY="155.0" prefHeight="80.0" prefWidth="249.0" text="No Lecture Selected" />
            <Button fx:id="SearchWeek" layoutX="303.0" layoutY="20.0" mnemonicParsing="false" onAction="#SearchWeek" text="Search" />
            <Button fx:id="SearchDate" layoutX="303.0" layoutY="59.0" mnemonicParsing="false" onAction="#SearchDate" text="Search" />
            <Button fx:id="SearchGroups" layoutX="303.0" layoutY="101.0" mnemonicParsing="false" onAction="#SearchGroups" text="Search" />
            <Button fx:id="SearchSubject" layoutX="698.0" layoutY="101.0" mnemonicParsing="false" onAction="#SearchSubject" text="Search" />
            <Button fx:id="SearchModule" layoutX="698.0" layoutY="59.0" mnemonicParsing="false" onAction="#SearchModule" text="Search" />
            <Button fx:id="SearchStaff" layoutX="698.0" layoutY="20.0" mnemonicParsing="false" onAction="#SearchStaff" text="Search" />
         </children>
      </AnchorPane>
   </children>
</VBox>
