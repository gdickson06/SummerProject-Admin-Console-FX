<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="800.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="application.LoginController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <AnchorPane prefHeight="773.0" prefWidth="800.0">
         <children>
            <Label layoutX="441.0" layoutY="14.0" text="Login page" />
            <Label layoutX="241.0" layoutY="211.0" text="Username" />
            <Label layoutX="241.0" layoutY="252.0" text="Password" />
            <PasswordField fx:id="PasswordText" layoutX="359.0" layoutY="248.0" prefHeight="25.0" prefWidth="317.0" />
            <TextField fx:id="UsernameText" layoutX="359.0" layoutY="207.0" prefHeight="25.0" prefWidth="317.0" />
            <Button fx:id="LoginButton" layoutX="333.0" layoutY="419.0" mnemonicParsing="false" onAction="#Login" text="Log in" />
            <Button fx:id="Clear" layoutX="570.0" layoutY="419.0" mnemonicParsing="false" onAction="#Clear" text="Clear" />
         </children>
      </AnchorPane>
   </children>
</VBox>
